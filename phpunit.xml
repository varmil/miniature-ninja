	<logging>
	  <log type="coverage-html" target="test_result/coverage-html" charset="UTF-8"
	       highlight="false" lowUpperBound="35" highLowerBound="70"/>
	  <log type="coverage-clover" target="test_result/coverage-clover.xml"/>
	</logging>
-->
</phpunit>